int *b;

void f( int *x, int *y ) {
    write(*x);  /* should write 4 */
    write(*y); /* 5 */
    *x = 23;
    *y = 55;
    write(*x); /* 23 */
    write(*y); /* 55 */
}

void main(void) {
    int a;
    int A[2];
    int *c;
    c = &A[1];
    b = &a;
    A[1] = 5;
    a = 4;
    5;
    write(*b); /* should write 4 */
    f( b, c );
    write(a); /* should write 23 */
    write(A[1]); /* 55 */
    writeln();
}
