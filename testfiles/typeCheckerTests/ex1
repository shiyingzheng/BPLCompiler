int fact(int n) {
    if (n == 0)
        return 1;
    else
        return n*fact(n-1);
}

void func(void) {
    return;
}

void main(void) {
    int x;
    int y;
    x = 1;
    while (x < 10) {
        write(x);
        write(fact(x));
        writeln();
        x = x + 1;
    }
    x = y = -1;
    x = x * y;
}

int y;
